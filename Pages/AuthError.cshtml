@page
@model woodgrovedemo.Pages.AuthErrorModel
@{
}

<h1 class="py-2">Authentication error</h1>

<p class="py-2">
    @Model.Message
</p>


@if(!string.IsNullOrEmpty(Model.UserMessage))
{
    <div class="alert alert-secondary" role="alert">
        <i class="bi bi-info-circle"></i> @Html.Raw(Model.UserMessage)
    </div>
}


